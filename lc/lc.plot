set datafile missing '?'
set yrange [0:1]
set term png truecolor size 3000,3000  noenhanced
set grid ytics
set grid xtics
set key bottom right
set key autotitle columnhead

set ytics 0,0.1,1
set xtics nomirror
set x2tics
set autoscale xfix
set autoscale x2fix
set xlabel 'Number of dialogues'
set x2label 'Number of utterances'


