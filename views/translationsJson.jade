doctype 5
html
	head
		title Translations Table 
		link(rel='stylesheet', href='/main.css')
	body#translations
		div #{lines.length} lines
		if locals.dataset
			pre
				| [ #{JSON.stringify({input: "", output: []})+"\n"} 
				for line in lines
					| #{", "+JSON.stringify({input: line.text, output: line.translations, is_correct: line.is_correct, timestamp: line.timestamp})+"\n"}
				| ] 
		else
			pre
				| #{JSON.stringify(lines,null,"\t")+"\n"}
